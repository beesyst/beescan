{
  "database": {
    "container_name": "secwebscan_postgres",
    "POSTGRES_USER": "secscan",
    "POSTGRES_PASSWORD": "securepass",
    "POSTGRES_DB": "secwebscan",
    "POSTGRES_PORT": 5432,
    "POSTGRES_HOST": "secwebscan_postgres"
  },
  "docker_network": "secwebscan_network",
  "scan_config": {
    "target_ip": "64.227.103.24",
    "target_domain": "honey.scanme.sh",
    "report_formats": ["terminal", "html"],
    "open_report": true,
    "clear_reports": true,
    "clear_logs": true,
    "clear_db": true,
    "report_theme": "dark"
  },
  "plugins": [
    {
      "name": "nmap",
      "enabled": true,
      "ip_required": true,
      "vhost_required": true,
      "category": "Network Security",
      "install": ["apt-get update", "apt-get install -y nmap"],
      "level": "easy",
      "levels": {
        "custom": {
          "ip_args": "-T4 -sT -Pn -n -p 22,25,80,443,996,8080,8443",
          "http_args": "-T4 -sT -Pn -n -p 80",
          "https_args": "-T4 -sT -Pn -n -p 443 --script ssl-cert,ssl-enum-ciphers",
          "args": "-T4 -sT -Pn -n"
        },
        "easy": {
          "ip_args": "-T4 -F -sS -Pn",
          "http_args": "-T4 -sS -Pn -p 80",
          "https_args": "-T4 -sS -Pn -p 443 --script ssl-cert,ssl-enum-ciphers",
          "args": "-T4 -sS -Pn"
        },
        "middle": {
          "ip_args": "-T4 -sS -sV -Pn --open --top-ports 1000",
          "http_args": "-T4 -sS -sV -Pn --open -p 80,8080,8000",
          "https_args": "-T4 -sS -sV -Pn --open -p 443,8443 --script ssl-cert,ssl-enum-ciphers",
          "args": "-T4 -sS -sV -Pn --open --top-ports 200"
        },
        "hard": {
          "ip_args": "-T4 -sS -sV -O -Pn --script \"default,safe,vuln\"",
          "http_args": "-T4 -sS -sV -O -Pn --script \"default,safe,vuln,http-fetch\" --script-args \"http-fetch.directory=/tmp\" -p 80",
          "https_args": "-T4 -sS -sV -O -Pn --script \"default,safe,vuln,ssl-cert,ssl-enum-ciphers,http-fetch\" --script-args \"http-fetch.directory=/tmp\" -p 443",
          "args": "-T4 -sS -sV -O -Pn --script \"default,safe,vuln\""
        },
        "extreme": {
          "ip_args": "-T4 -p- -sS -sV -O -Pn --script \"default,safe,vuln,discovery and not targets-asn and not eap-info and not http-fetch and not http-passwd and not http-robtex-shared-ns and not hostmap-robtex and not http-vuln-cve2013-7091\" --script-args \"http-fetch.directory=/tmp\" --max-retries 2 --host-timeout 5m",
          "http_args": "-T4 -p 80 -sS -sV -O -Pn --script \"default,safe,vuln,discovery and not targets-asn and not eap-info and not http-fetch and not http-passwd and not http-robtex-shared-ns and not hostmap-robtex and not http-vuln-cve2013-7091\" --script-args \"http-fetch.directory=/tmp\" --max-retries 2 --host-timeout 5m",
          "https_args": "-T4 -p 443 -sS -sV -O -Pn --script \"default,safe,vuln,discovery,ssl-cert,ssl-enum-ciphers and not targets-asn and not eap-info and not http-fetch and not http-passwd and not http-robtex-shared-ns and not hostmap-robtex and not http-vuln-cve2013-7091\" --script-args \"http-fetch.directory=/tmp\" --max-retries 2 --host-timeout 5m",
          "args": "-T4 -p- -sS -sV -O -Pn --script \"default,safe,vuln,discovery and not targets-asn and not eap-info and not http-fetch and not http-passwd and not http-robtex-shared-ns and not hostmap-robtex and not http-vuln-cve2013-7091\" --script-args \"http-fetch.directory=/tmp\" --max-retries 2 --host-timeout 5m"
        }
      }
    },
    {
      "name": "nikto",
      "enabled": false,
      "ip_required": true,
      "vhost_required": true,
      "category": "Application Security",
      "install": [
        "apt-get update",
        "apt-get install -y git perl libnet-ssleay-perl libwhisker2-perl liblwp-useragent-determined-perl",
        "cd /opt && git clone https://github.com/sullo/nikto.git",
        "echo '#!/bin/bash' > /usr/local/bin/nikto",
        "echo 'cd /opt/nikto/program' >> /usr/local/bin/nikto",
        "echo 'perl nikto.pl \"$@\"' >> /usr/local/bin/nikto",
        "chmod +x /usr/local/bin/nikto"
      ],
      "level": "easy",
      "levels": {
        "custom": {
          "args": "-Tuning 2345 -Display V -Plugins tests"
        },
        "easy": {
          "args": "-Tuning 2,4,5 -maxtime 60 -nointeractive -Display P"
        },
        "middle": {
          "args": "-Tuning 12345 -Display V -nointeractive -followredirects"
        },
        "hard": {
          "args": "-Tuning 1234567890abc -Display V -maxtime 120 -followredirects -evasion 1,2,3 -ssl"
        },
        "extreme": {
          "args": "-Tuning 1234567890abcx -Display VE -maxtime 300 -followredirects -evasion 1,2,3,4,5,6,7,8 -ssl -Plugins ALL"
        }
      }
    },
    {
      "name": "dig",
      "enabled": false,
      "ip_required": false,
      "vhost_required": true,
      "category": "DNS Health",
      "install": ["apt-get update", "apt-get install -y dnsutils"],
      "command": "",
      "output": "results/dig.json",
      "parser": "python",
      "level": "easy",
      "levels": {
        "custom": {
          "args": ""
        },
        "easy": {
          "args": "A"
        },
        "middle": {
          "args": "A MX NS TXT"
        },
        "hard": {
          "args": "A MX NS TXT CNAME SOA"
        },
        "extreme": {
          "args": "A MX NS TXT CNAME SOA SPF ANY"
        }
      }
    },
    {
      "name": "nuclei",
      "enabled": false,
      "ip_required": false,
      "vhost_required": true,
      "category": "Vulnerability Scan",
      "install": [
        "go install -v github.com/projectdiscovery/nuclei/v3/cmd/nuclei@latest",
        "cp /root/go/bin/nuclei /usr/local/bin/nuclei"
      ],
      "command": "nuclei {args} -u http://{target} -jsonl -o {stdout} -t /root/nuclei-templates",
      "output": "results/nuclei.json",
      "parser": "json",
      "level": "easy",
      "levels": {
        "custom": {
          "args": "-t /nuclei-templates -severity medium,high"
        },
        "easy": {
          "args": "-severity low,medium"
        },
        "middle": {
          "args": "-severity medium,high -rate-limit 50"
        },
        "hard": {
          "args": "-severity high,critical -rate-limit 100"
        },
        "extreme": {
          "args": "-severity critical -rate-limit 200 -c 100"
        }
      }
    },
    {
      "name": "amass",
      "category": "discovery",
      "command": "amass enum -d {target} -json /results/amass.json",
      "output": "results/amass.json",
      "enabled": false
    },
    {
      "name": "subfinder",
      "category": "discovery",
      "command": "subfinder -d {target} -oJ /results/subfinder.json",
      "output": "results/subfinder.json",
      "enabled": false
    },
    {
      "name": "httpx",
      "category": "scanning",
      "command": "httpx -l /data/targets.txt -json -o /results/httpx.json",
      "output": "results/httpx.json",
      "enabled": false
    },
    {
      "name": "naabu",
      "category": "ports",
      "command": "naabu -host {target} -json -o /results/naabu.json",
      "output": "results/naabu.json",
      "enabled": false
    },
    {
      "name": "ffuf",
      "category": "fuzzing",
      "command": "ffuf -u http://{target}/FUZZ -w /wordlists/common.txt -of json -o /results/ffuf.json",
      "output": "results/ffuf.json",
      "enabled": false
    }
  ]
}
